{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Kydras Echo",
  "version": "0.1.0",
  "identifier": "com.kydras.echo",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://127.0.0.1:8000",
    "beforeDevCommand": "",
    "beforeBuildCommand": ""
  },
  "app": {
    "windows": [
      {
        "title": "Kydras Echo",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "identifier": "com.kydras.echo"
  }
}